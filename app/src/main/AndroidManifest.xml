<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.maxspahn.studentbnb">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SearchRoomActivity"></activity>
        <activity android:name=".RoomReservationActivity"></activity>
        <activity android:name=".ProfileActivity"></activity>
        <activity android:name=".InfoActivity"></activity>
        <activity android:name=".TripsActivity"></activity>
        <activity android:name=".EvaluationsActivity"></activity>
    </application>

</manifest>